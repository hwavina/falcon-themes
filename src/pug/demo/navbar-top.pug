extends ../layouts/LayoutTheme
include ../mixins/home/dashboard/Dashboard

append variables
  - var CWD = '../'
  - var pageTree = ['navbar-top']

prepend scripts
  script(src=`${CWD}vendors/echarts/echarts.min.js`)
  script(src=`${CWD}vendors/progressbar/progressbar.min.js`)

block navbarTop

block beforeContent
  +NavbarTop({
    toggleID: 'navbarStandard',
    links: true,
    search: false,
    changelog: true
  }).navbar-expand-lg

block content
  +Dashboard